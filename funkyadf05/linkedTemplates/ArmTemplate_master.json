{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "funkyadf05"
		},
		"ADdfByExample_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ADdfByExample'"
		},
		"AdfByExampleSa02_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AdfByExampleSa02'"
		},
		"adfbyexamplesa_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'adfbyexamplesa'"
		},
		"ADdfByExample_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "migserever02.database.windows.net"
		},
		"ADdfByExample_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "AdfByExample"
		},
		"ADdfByExample_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "mydb01"
		},
		"AdfByExampleSa02_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://snapseqdls05.dfs.core.windows.net/"
		},
		"adfbyexamplesa_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://snapseqdls05.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "funkyadf05_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADdfByExample_password": {
						"value": "[parameters('ADdfByExample_password')]"
					},
					"AdfByExampleSa02_accountKey": {
						"value": "[parameters('AdfByExampleSa02_accountKey')]"
					},
					"adfbyexamplesa_accountKey": {
						"value": "[parameters('adfbyexamplesa_accountKey')]"
					},
					"ADdfByExample_properties_typeProperties_server": {
						"value": "[parameters('ADdfByExample_properties_typeProperties_server')]"
					},
					"ADdfByExample_properties_typeProperties_database": {
						"value": "[parameters('ADdfByExample_properties_typeProperties_database')]"
					},
					"ADdfByExample_properties_typeProperties_userName": {
						"value": "[parameters('ADdfByExample_properties_typeProperties_userName')]"
					},
					"AdfByExampleSa02_properties_typeProperties_url": {
						"value": "[parameters('AdfByExampleSa02_properties_typeProperties_url')]"
					},
					"adfbyexamplesa_properties_typeProperties_url": {
						"value": "[parameters('adfbyexamplesa_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}